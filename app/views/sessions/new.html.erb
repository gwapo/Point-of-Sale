<h1>Log In</h1>
<div id="item_container">
<%= form_tag sessions_path do  %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <label>&nbsp;</label>
    <%= submit_tag "Log In" %>
  </div>
<% end %>
</div>

